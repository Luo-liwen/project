@model MyMVCDemoo.Models.ViewModels.Member.AddressEditModel

@{
    ViewData["Title"] = "Edit";
}

<h1>Edit</h1>

<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            
            <div class="form-group">
                <label asp-for="姓名" class="control-label"></label>
                <input asp-for="姓名" class="form-control" />
                <span asp-validation-for="姓名" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="电话" class="control-label"></label>
                <input asp-for="电话" class="form-control" />
                <span asp-validation-for="电话" class="text-danger"></span>
            </div>
            <div class="form-group">
            @Html.HiddenFor(m=>m.Id)
                <input type="submit" value="确定" class="btn btn-primary myconfirm" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
