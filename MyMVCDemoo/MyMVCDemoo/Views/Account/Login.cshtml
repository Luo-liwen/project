@model MyMVCDemoo.Models.ViewModels.Account.LoginModel

@{
    ViewData["Title"] = "Login";
}

<h1>Login</h1>

<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Login">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Qq号" class="control-label"></label>
                <input asp-for="Qq号" class="form-control" />
                <span asp-validation-for="Qq号" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="密码" class="control-label"></label>
                <input asp-for="密码" class="form-control" />
                <span asp-validation-for="密码" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="登录" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Register" >注册</a>
</div>

